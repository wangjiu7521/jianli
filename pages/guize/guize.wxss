.container{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  font-family: "幼圆";
  /* align-items: center; */
}

.lable{
  margin-top: 50rpx;
  width: 100%;
  min-height: 75rpx;
  background-color: #faf9f9;
  display: flex;
  flex-wrap: wrap;
  align-items: center; 
}

.lable-text{
  margin-left: 65rpx;
  color: #fcaf17;
  font-size: 30rpx;
  font-weight:bold;
}
.lable-text-small{
  margin-top: 10rpx;
  color: #fcaf17;
  font-size: 20rpx;
  font-weight:bold;
}

.detail{
  margin-top: 25rpx;
  width: 650rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  padding-right: 60rpx;
  background-color: #faf9f9;
  border-radius: 7rpx;
  display: flex;
  flex-flow: column;
  flex-wrap: wrap;
  box-sizing: border-box; 
}
.detail-text{
  margin-left: 60rpx;
  font-size: 24rpx;
  line-height: 40rpx;
}
.detail-text-shorttab{
  margin-left: 15rpx;
}

.lable-text-detail{
  margin-left: 65rpx;
  margin-right: 60rpx;
  font-size: 24rpx;
  line-height: 40rpx;
}

.kongbai{
  width: 100%;
  height: 145rpx;
}